  "scripts": {
    "typecheck": "tsc --noEmit",
    "start": "CI_BUILD_NUMBER=${CI_BUILD_NUMBER:-`grep CI_BUILD_NUMBER .env | cut -d '=' -f2`} REACT_APP_CI_BUILD_NUMBER=${CI_BUILD_NUMBER:-`grep CI_BUILD_NUMBER .env | cut -d '=' -f2`} run-p run:react run:css",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0",
    "build": "npm run build:tailwind && CI_BUILD_NUMBER=${CI_BUILD_NUMBER:-`grep CI_BUILD_NUMBER .env | cut -d '=' -f2`} REACT_APP_CI_BUILD_NUMBER=${CI_BUILD_NUMBER:-`grep CI_BUILD_NUMBER .env | cut -d '=' -f2`} IOS_DEVELOPMENT_TEAM=${IOS_DEVELOPMENT_TEAM:-`grep IOS_DEVELOPMENT_TEAM .env | cut -d '=' -f2`} react-scripts build",
    "test": "react-scripts test --coverage --reporters=default --reporters=jest-junit",
    "eject": "react-scripts eject",
    "prettier": "prettier --write './src/**/*.{ts,tsx,js,scss,css,html}'",
    "chromatic": "npm run build:tailwind && npx chromatic storybook-static/",
    "build:android": "REACT_APP_CI_BUILD_NUMBER=${CI_BUILD_NUMBER:-`grep CI_BUILD_NUMBER .env | cut -d '=' -f2`} ionic cap build android",
    "build:ios": "CI_BUILD_NUMBER=${CI_BUILD_NUMBER:-`grep CI_BUILD_NUMBER .env | cut -d '=' -f2`} REACT_APP_CI_BUILD_NUMBER=${CI_BUILD_NUMBER:-`grep CI_BUILD_NUMBER .env | cut -d '=' -f2`} IOS_DEVELOPMENT_TEAM=${IOS_DEVELOPMENT_TEAM:-`grep IOS_DEVELOPMENT_TEAM .env | cut -d '=' -f2`} ionic cap build ios",
    "build:tailwind": "postcss ./src/assets/tailwind.css -o ./src/assets/compiled.css",
    "generate": "graphql-codegen --require dotenv/config --config ./codegen.yml ",
    "generate:icons": "node scripts/icons-generator.mjs",
    "run:android": "REACT_APP_CI_BUILD_NUMBER=${CI_BUILD_NUMBER:-`grep CI_BUILD_NUMBER .env | cut -d '=' -f2`} ionic cap run android -l --external",
    "run:css": "postcss src/assets/tailwind.css -o src/assets/compiled.css -w",
    "run:ios": "CI_BUILD_NUMBER=${CI_BUILD_NUMBER:-`grep CI_BUILD_NUMBER .env | cut -d '=' -f2`} REACT_APP_CI_BUILD_NUMBER=${CI_BUILD_NUMBER:-`grep CI_BUILD_NUMBER .env | cut -d '=' -f2`} IOS_DEVELOPMENT_TEAM=${IOS_DEVELOPMENT_TEAM:-`grep IOS_DEVELOPMENT_TEAM .env | cut -d '=' -f2`} ionic cap run ios -l --external",
    "run:react": "react-scripts start",
    "build-storybook": "build-storybook -s public",
    "storybook": "run-p run:css run:storybook",
    "run:storybook": "start-storybook -p 6006 -s public",
    "prepare": "cd ../../ && husky install"
  },
